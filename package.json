{
  "name": "@crishellco/vue-breeze",
  "version": "0.0.0-semantically-released",
  "private": true,
  "description": "A Vue 2 Tailwind UI Component Library",
  "author": "Chris Mitchell (@crishellco)",
  "scripts": {
    "build": "vue-cli-service build --target lib src/main.js",
    "test:unit": "vue-cli-service test:unit --passWithNoTests",
    "build-storybook": "build-storybook",
    "lint:js": "vue-cli-service lint src ",
    "lint:js:fix": "yarn lint --fix",
    "storybook": "start-storybook -p 6006",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*"
  },
  "main": "./dist/vue-breeze.umd.js",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "autoprefixer": "^9",
    "core-js": "^3.6.5",
    "postcss": "^7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "v-click-outside": "^3.1.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@commitlint/cli": "16.1.0",
    "@form8ion/commitlint-config": "1.0.28",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.4.14",
    "@storybook/vue": "^6.4.14",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.3.0",
    "husky": "7.0.4",
    "js-beautify": "1.14.0",
    "lint-staged": "12.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "semantic-release": "19.0.2",
    "vue-cli-plugin-tailwind": "~2.2.18",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
